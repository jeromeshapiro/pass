	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/build/pass",
			"preLaunchTask": "make",
			"internalConsoleOptions": "openOnSessionStart",
			"logging": {
				"moduleLoad": false,
				"programOutput": true,
				"trace": false
			},
			"showDisplayString": false,
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": false,
			"osx": {
				"MIMode": "lldb"
			}
		}
	]
}